@page
@model RReviews.Pages.GalleryModel

@{
    ViewData["Title"] = "Gallery";
}

<div class="row">`
    <div class="col container">
        <div class="col-10">
            @foreach (var item in Model.ImageList)
            {
                <img class="float-left p-2" src="@item" height="150" />
            }
        </div>
    </div>
</div>